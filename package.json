{
  "name": "@tspower/reflection",
  "version": "1.1.3",
  "description": "Easy javascript object reflection",
  "main": "./dist/src/reflection.js",
  "types": "./dist/src/reflection.d.ts",
  "scripts": {
    "publish": "npm publish --access public",
    "setup": "npm install",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test-only": "mocha --require ts-node/register dist/test/**/*.ts",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "clean": "shx rm -rf node_modules/ dist/ docs/",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll"
  },
  "author": "Luigi Senzamici (Pegaso Professional)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "shx": "^0.3.2",
    "ts-node": "^7.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1"
  },
  "directories": {
    "lib": "dist/src"
  },
  "keywords": [
    "javscript",
    "object",
    "reflection"
  ],
  "publishConfig": {
    "access": "public"
  }
}
